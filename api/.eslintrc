{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": false,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended",
        "plugin:prettier/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "node/no-unsupported-features/es-syntax": [
            "error",
            {
                "version": ">=13.0.0",
                "ignores": [
                    "modules"
                ]
            }
        ],
        "no-unused-vars": [
            "warn",
            {
                "vars": "all",
                "args": "after-used",
                "ignoreRestSiblings": false
            }
        ]
    }
}